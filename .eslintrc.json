
{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": "off",        
        "no-console":"off",
        "import/extensions": 0
    },
    "import/no-extraneous-dependencies": [
        "error", { 
            "devDependencies": false, 
            "optionalDependencies": false, 
            "peerDependencies": false 
        }
    ]
}
